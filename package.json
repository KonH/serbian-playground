{
  "name": "serbian-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm run test:watch\" \"vue-cli-service serve\"",
    "test:watch": "jest --watch",
    "build": "vue-cli-service build",
    "test": "jest test",
    "lint": "vue-cli-service lint",
    "convert_verbs": "ts-node src/scripts/convert_verbs.ts",
    "deploy": "npm run build && shx rm -rf ../konh.github.io/custom/serbian-playground/* && shx cp -r dist/* ../konh.github.io/custom/serbian-playground"
  },
  "dependencies": {
    "bootstrap": "~5.3.0",
    "bootstrap-vue-3": "~0.5.1",
    "core-js": "~3.37.0",
    "papaparse": "~5.4.0",
    "vue": "~3.4.0",
    "vue-i18n": "~9.13.0",
    "vuex": "~4.1.0"
  },
  "devDependencies": {
    "@types/jest": "~29.5.0",
    "@types/node": "~20.12.0",
    "@types/papaparse": "~5.3.0",
    "@typescript-eslint/eslint-plugin": "~7.8.0",
    "@typescript-eslint/parser": "~7.8.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "~13.0.0",
    "concurrently": "~8.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "~9.25.0",
    "jest": "~29.7.0",
    "raw-loader": "~4.0.0",
    "shx": "~0.3.0",
    "ts-jest": "~29.1.0",
    "ts-node": "~10.9.0",
    "typescript": "~5.4.5",
    "vue-template-compiler": "~2.7.16"
  }
}
